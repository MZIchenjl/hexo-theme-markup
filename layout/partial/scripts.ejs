<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/<%= theme.jquery_version || '3.5.1' %>/jquery.min.js"></script>
<% if (theme.plugins.fancybox.enable) { %>
<script src="//cdnjs.cloudflare.com/ajax/libs/fancybox/<%= theme.plugins.fancybox.version || '3.5.7' %>/jquery.fancybox.min.js"></script>
<% } %>
<% if (theme.plugins.mermaid.enable) { %>
<script src="//cdnjs.cloudflare.com/ajax/libs/mermaid/<%= theme.plugins.mermaid.version || '8.5.1' %>/mermaid.min.js"></script>
<script>
    if (window.mermaid) {
        mermaid.initialize(<%= theme.plugins.mermaid.options || '{}' %>);
    }
</script>
<% } %>
<%- js('js/index.js') %>
<% if (theme.plugins.google_analytics) { %>
<script async src="//www.googletagmanager.com/gtag/js?id=<%= theme.plugins.google_analytics %>"></script>
<%- partial('plugin/google-analytics') %>
<% } %>
<% if (theme.plugins.baidu_analytics) { %>
<%- partial('plugin/baidu-analytics') %>
<% } %>
<% if (theme.plugins.mathjax.enable) { %>
<script>
    MathJax = {
        tex: {
            inlineMath: [['$', '$'], ['\\(', '\\)']]
        }
    };
</script>
<script id="MathJax-script" async src="//cdnjs.cloudflare.com/ajax/libs/mathjax/<%= theme.plugins.mathjax.version || '3.0.5' %>/es5/tex-mml-chtml.js"></script>
<% } %>
