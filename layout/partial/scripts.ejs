<% if (theme.plugins.google_analytics) { %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= theme.plugins.google_analytics %>"></script>
<%- partial('plugin/google-analytics') %>
<% } %>
<% if (theme.plugins.baidu_analytics) { %>
<%- partial('plugin/baidu-analytics') %>
<% } %>
<% if (theme.plugins.mathjax) { %>
<script type="text/x-mathjax-config">
    window.MathJax.Hub.Config({ tex2jax: { inlineMath: [['$','$'], ['\\(','\\)']] } });
</script>
<%- js('https://cdnjs.cdn.miao.su/mathjax/2.7.2/MathJax.js?config=TeX-MML-AM_CHTML') %>
<% } %>
